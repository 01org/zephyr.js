{
    "module": "spi",
    "require": "spi",
    "depends": ["buffer"],
    "zephyr_conf": {
        "all": [
            "CONFIG_SPI=y"
        ],
        "frdm_k64f": [
            "CONFIG_SPI_MCUX_DSPI=y"
        ]
    },
    "zjs_config": ["-DBUILD_MODULE_SPI"],
    "src": ["zjs_spi.c"],
    "header": ["zjs_spi.h"],
    "init": ["zjs_spi_init"],
    "cleanup": ["zjs_spi_cleanup"]
}
