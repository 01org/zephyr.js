{
    "module": "i2c",
    "require": "i2c",
    "depends": ["buffer", "i2c_frdm", "i2c_a101"],
    "zephyr_conf": {
        "all": [
            "CONFIG_I2C=y"
        ]
    },
    "zjs_config": ["-DBUILD_MODULE_I2C"],
