{
    "module": "net-config",
    "require": "net-config",
    "depends": ["events"],
    "zjs_config": ["-DBUILD_MODULE_NET_CONFIG"],
    "zephyr_conf": {
        "all": [
            "CONFIG_NETWORKING=y",
            "CONFIG_NET_MGMT=y",
            "CONFIG_NET_MGMT_EVENT=y",
            "CONFIG_NET_DHCPV4=y"
        ]
    },
    "header": ["zjs_net_config.h"]
}
