{
    "module": "grove_lcd_arc",
    "require": "grove_lcd",
    "depends": ["ipm_arc", "i2c_arc"],
    "targets": ["arc"],
    "zephyr_conf": {
        "all": [
            "CONFIG_I2C=y",
            "CONFIG_GROVE=y",
            "CONFIG_GROVE_LCD_RGB=y",
            "CONFIG_GROVE_LCD_RGB_INIT_PRIORITY=90"
        ]
    },
    "zjs_config": ["-DBUILD_MODULE_GROVE_LCD"]
}
